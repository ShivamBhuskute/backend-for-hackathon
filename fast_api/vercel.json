{
    "version": 2,
    "builds": [
      {
        "src": "fast_api.py",
        "use": "@vercel/python"
      }
    ],
    "pythonVersion": "3.10",
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/fast_api.py"
      }
    ],
    "env": {
      "APP_MODULE": "fast_api:app"
    }
  }