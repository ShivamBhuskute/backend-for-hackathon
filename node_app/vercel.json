{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/aI/(.*)",
      "dest": "/api/index.js"  
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"  
    }
  ],
  "rewrites": [
    {
      "source": "/old-path",
      "destination": "/new-path"
    }
  ]
}